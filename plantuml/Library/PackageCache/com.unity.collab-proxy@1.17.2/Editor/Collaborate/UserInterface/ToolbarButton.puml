@startuml
class ToolbarButton {
    + ToolbarButton()
    + <<override>> OnGUI(rect:Rect) : void
    + Update() : void
}
enum ToolbarButtonState {
    NeedToEnableCollab,
    UpToDate,
    Conflict,
    OperationError,
    ServerHasChanges,
    FilesToPush,
    InProgress,
    Disabled,
    Offline,
    Plastic,
}
SubToolbar <|-- ToolbarButton
ToolbarButton +-- ToolbarButtonState
@enduml
